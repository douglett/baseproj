# This is Git's per-user configuration file.
[user]
	name = doug@xxx
	email = doug@totem9.net
[core]
	excludesfile = ~/.gitignore_global
[push]
	default = matching
[format]
	pretty = pretty = %C(green)(%cr) %C(yellow)%h %C(cyan)%an %C(red)%d %C(green)(%ci)%Creset\n   %s
[credential]
	helper = cache --timeout=14400  # timeout in 4 hours
